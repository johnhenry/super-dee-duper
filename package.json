{
  "name": "dupe-scanner",
  "version": "1.0.0",
  "type": "module",
  "description": "A tool for finding and managing duplicate files",
  "main": "cli.mjs",
  "bin": {
    "dupe-scan": "./cli.mjs"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "start": "node cli.mjs scan",
    "generate": "node cli.mjs generate-test"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".mjs"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.mjs$": "$1"
    }
  },
  "keywords": [
    "duplicate",
    "files",
    "scanner",
    "cleanup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "mime-types": "^2.1.35",
    "open": "^9.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0"
  }
}